{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "env:sync": "git submodule update --init --remote",
    "env:local": "sops -d git.environment-variables/application.next-js/local.sops.env > .env.local",
    "env:dev": "sops -d git.environment-variables/application.next-js/dev.sops.env > .env.development",
    "env:prod": "sops -d git.environment-variables/application.next-js/prod.sops.env > .env.production",
    "setup": "pnpm run env:sync && pnpm run env:local",
    "dev": "pnpm run env:dev && cross-env npx env-cmd -f .env.development next dev",
    "dev:local": "pnpm run env:local && cross-env npx env-cmd -f .env.local next dev",
    "dev:prod": "pnpm run env:prod && cross-env npx env-cmd -f .env.production next dev",
    "build:dev": "pnpm run env:dev && cross-env npx env-cmd -f .env.development next build",
    "build:prod": "pnpm run env:prod && cross-env npx env-cmd -f .env.production next build",
    "start:dev": "pnpm run env:dev && cross-env npx env-cmd -f .env.development next start",
    "start:prod": "pnpm run env:prod && cross-env npx env-cmd -f .env.production next start",
    "build": "next build",
    "start": "next start",
    "ci:tsc": "tsc",
    "ci:lint": "next lint",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "prepare": "husky",
    "optimize:banners": "node scripts/optimize-banner-images.mjs"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.6.0",
    "@jest/globals": "^30.0.5",
    "@next/third-parties": "^15.3.0",
    "@radix-ui/react-slot": "^1.2.3",
    "@tanstack/react-query": "^5.49.0",
    "@tanstack/react-query-devtools": "^5.76.1",
    "@testing-library/jest-dom": "^6.4.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cross-env": "^7.0.3",
    "crypto": "^1.0.1",
    "date-fns": "^4.1.0",
    "embla-carousel-react": "^8.6.0",
    "framer-motion": "^11.1.7",
    "html2canvas": "^1.4.1",
    "jose": "^5.9.3",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.511.0",
    "next": "14.2.35",
    "next-auth": "^4.24.7",
    "react": "^18",
    "react-datepicker": "^7.6.0",
    "react-daum-postcode": "^3.1.3",
    "react-dom": "^18",
    "react-hook-form": "^7.52.0",
    "react-loading-skeleton": "^3.5.0",
    "recharts": "^3.7.0",
    "sharp": "^0.34.1",
    "tailwind-merge": "^3.3.0",
    "tailwind-scrollbar-hide": "^1.1.7",
    "tailwindcss-animate": "^1.0.7",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "uuid": "^11.0.5",
    "vaul": "^1.1.2",
    "yup": "^1.4.0",
    "zustand": "^4.5.4"
  },
  "devDependencies": {
    "@svgr/webpack": "^8.1.0",
    "@testing-library/dom": "^9.3.4",
    "@testing-library/react": "^14.2.1",
    "@types/jest": "^29.5.12",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/testing-library__jest-dom": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^7.17.0",
    "autoprefixer": "^10.0.1",
    "dotenv": "^16.4.5",
    "eslint": "^8",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-next": "^14.1.3",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-storybook": "^0.8.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^16.2.7",
    "postcss": "^8",
    "prettier": "3.2.5",
    "shx": "^0.4.0",
    "tailwindcss": "^3.3.0",
    "typescript": "^5.4.0"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{js,jsx,json,css,md}": [
      "prettier --write"
    ]
  }
}
